EXPLAIN (COSTS OFF)
WITH dept_count AS 
(
SELECT deptno, COUNT(*) AS dept_count
FROM emp
GROUP BY deptno
)
SELECT e.ename AS Employee_Name,
dc.dept_count AS "Employee in Same Dept"
FROM emp e,
dept_count dc
WHERE e.deptno = dc.deptno
ORDER BY e.deptno, e.ename;
